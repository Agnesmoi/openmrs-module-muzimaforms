<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.html5forms">
    <class name="HTML5Form" table="html5forms_form">
        <id name="id" type="java.lang.Integer" column="form_id"/>
        <one-to-one name="form" class="org.openmrs.Form" foreign-key="form_id"/>
        <one-to-one name="xform" class="org.openmrs.module.xforms.Xform" foreign-key="form_id"/>
    </class>
</hibernate-mapping>